clf;clc;clear;
h=6.626e-34;m=9.1e-31;v=1;s=0.5;k=1.38e-23;u=1;e=1.6e-19;c=3e8;
E=0:0.001:2;
T=[10^8 10^9]
cn=(2*s*4*3.14*v)/((h^3)*(c^3))
for j=1:length(T)
b=1/(k*T(j))
for i=1:length(E)
g(i)=cn*(E(i)^2);
n(j,i)=1/(exp((E(i)-u)*10^6*b*e)+1);
f(j,i)=g(i)*n(j,i)
end
subplot(1,3,1)
plot(E',g)
xlabel('g(E)')
subplot(1,3,2)
plot(E',n)
subplot(1,3,3)
plot(E',f)
end
